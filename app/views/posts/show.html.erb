<h3>スケジュール確認</h3>


<table>
  <tr>
    <th>タイトル</th>
    <td><%= @post.title %></td>
  </tr>
  <tr>
    <th>開始日</th>
    <td><%= @post.start %></td>
  </tr>
  <tr>
    <th>終了日</th>
    <td><%= @post.finish %></td>
  </tr>
  <tr>
    <th>終日</th>
    <td><%= @post.day_all %></td>
  </tr>
</table>

<div>
  <ul>
    <li><%= link_to "スケジュール一覧に戻る", :posts %></li>
  </ul>
</div>